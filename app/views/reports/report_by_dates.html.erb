<h1>Reports#report_by_dates</h1>
<div class="col-6">
  <canvas id="myChart"></canvas>
</div>
<script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
<script>
  const ctx = document.getElementById('myChart');
  new Chart(ctx, {
  type: 'line',
  data: {
  labels: <%= raw @labels %>,
  datasets: [{
  label: '# of Votes',
  data: <%= raw @data %>,
  borderWidth: 1
  }]
  }
  });
</script>